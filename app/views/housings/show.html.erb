<h1>Showing listing <%= @housing.title %></h1>
<h2><%= flash[:notice] %></h2>
<script type="text/javascript">
	<%= show_housing_listing(@address) %>
</script>
<table>
	<tr>
		<th>Title</th>
		<th>Description</th>
		<th>Pics</th>
		<th></th>
	</tr>
	<tr>
		<td><%= @housing.title %></td>
		<td><%= @housing.description %></td>
		<td>
			<% @housing.housing_images.each do |image| %>
				<%= image_tag image.photo.url(:small) %>
			<% end %>
		</td>
		<td></td>
	</tr>

</table>
<div id="map" style="width: 547px; height: 320px; border: 1px solid #777; overflow: hidden;">
</div>
<br />


<%= link_to 'New listing', new_housing_path %>
