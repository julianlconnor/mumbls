<p>List of current users</p>
<table>
<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
	<td><%= image_tag user.avatar.url(:thumb) %>
  </tr>
<% end %>
</table>
<% if current_user %>
	<%= link_to 'Listings', listings_path %>
<% else %>
	<p>Please Login in order to view listings</p>
<% end %>


